{
  "rewrites": [
    {
      "source": "/styles.css",
      "destination": "/public/styles.css"
    },
    {
      "source": "/script.js",
      "destination": "/public/script.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
